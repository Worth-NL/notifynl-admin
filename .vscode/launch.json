{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Admin",
      "type": "debugpy",
      "request": "launch",
      "envFile": "${workspaceFolder}/.env",
      "module": "flask",
      "args": [
        "run",
        "--port",
        "6012",
        "--debug"
      ],
      "autoStartBrowser": true,
      "consoleName": "Admin",
      "preLaunchTask": "Install Admin local LandRegistry-frontend-jinja"
    },
    {
      "name": "Admin watch",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "watch"],
      "preLaunchTask": "NPM Admin build"
    }
  ],
  "compounds": [
    {
      "name": "Notify Admin",
      "configurations": [
        "Admin",
        "Admin watch"
      ],
      "stopAll": true
    }
  ]
}
